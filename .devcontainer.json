{
  "name": "docs.github.com",
  "service": "container-doc",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "cSpell.language": ",en"
  },
  "postCreateCommand": "npm install && npm start",
  "forwardPorts": [
    4000
  ],
  "extensions": [
    "yzhang.markdown-all-in-one",
    "streetsidesoftware.code-spell-checker"
  ]
}
